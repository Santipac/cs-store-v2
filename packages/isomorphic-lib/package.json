			"types": "./dist/products/index.d.ts",
			"import": "./dist/products/index.js"
		}
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"check-types": "tsc --noEmit"
	},
	"dependencies": {
		"zod": "^3.25.16"
	},
	"devDependencies": {
		"typescript": "^5.8.2"

{
	"name": "@cs-store/isomorphic-lib",
	"version": "0.1.0",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./auth": {
			"types": "./dist/auth/index.d.ts",
			"import": "./dist/auth/index.js"
		},
		"./common": {
			"types": "./dist/common/index.d.ts",
			"import": "./dist/common/index.js"
		},
